DROP PROCEDURE `SbLo_MovimientoDetGuardar`;
CREATE DEFINER=`root`@`localhost` PROCEDURE `SbLo_MovimientoDetGuardar`(IN `var_Hash` VARCHAR(255), IN `var_Producto` VARCHAR(255), IN `var_Cantidad` FLOAT, IN `var_TieneIgv` BIT, IN `var_Precio` FLOAT, IN `var_ISC` FLOAT, IN `var_FLETE` FLOAT, IN `var_IdLote` INT, IN `var_FechaVen` DATE, IN `var_PesoTotal` VARCHAR(255)) NOT DETERMINISTIC CONTAINS SQL SQL SECURITY DEFINER BEGIN






	INSERT INTO Lo_MovimientoDetalle(hashMovimiento, IdProducto, Cantidad, TieneIgv, Precio, ISC, FLETE, IdLote, FechaVen, PesoTotal)







	VALUES(







	var_Hash,







	(SELECT IdProducto FROM Gen_Producto WHERE Producto = var_Producto),







	var_Cantidad,







	var_TieneIgv,







	var_Precio,



var_ISC ,

var_FLETE ,

var_IdLote ,

var_FechaVen,

var_PesoTotal



	);







END